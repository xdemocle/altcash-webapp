{
  "name": "altcash-frontend",
  "description": "A showcase of Next.js + React + Typescript + Apollo + GraphQL.",
  "version": "2.0.1",
  "license": "CC BY-NC 4.0",
  "author": "Rocco R. <im@rocco.me>",
  "scripts": {
    "dev": "next dev -p 3000",
    "dev:inspect": "NODE_OPTIONS='--inspect' next dev -p 3000",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "fix-code": "prettier --write $PWD/'src/**/*.{jsx,ts,tsx}'",
    "lint": "eslint ./src/ --ext .jsx,.ts,.tsx",
    "node-app-host": "node server 80",
    "opn-build": "opennextjs-cloudflare build",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "upload": "opennextjs-cloudflare build && opennextjs-cloudflare upload",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@apollo/client": "4.0.7",
    "@emotion/react": "^11.14.0",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "7.3.4",
    "@mui/lab": "^7.0.1-beta.18",
    "@mui/material": "7.3.4",
    "@next/eslint-plugin-next": "^15.5.6",
    "@paystack/inline-js": "^2.22.7",
    "apollo3-cache-persist": "^0.15.0",
    "axios": "^1.12.2",
    "clsx": "^2.1.1",
    "crypto-random-string": "^5.0.0",
    "cryptocurrency-icons": "^0.18.1",
    "eslint": "^9.38.0",
    "eslint-config-next": "^15.5.6",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "graphql": "^16.11.0",
    "http-proxy-middleware": "^3.0.5",
    "localforage": "^1.10.0",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "moment-timezone": "^0.6.0",
    "next": "^15.5.6",
    "prettier": "^3.6.2",
    "react": "19.2.0",
    "react-cookie-consent": "^9.0.0",
    "react-dom": "19.2.0",
    "react-google-recaptcha": "^3.1.0",
    "react-moment": "^1.1.3",
    "react-number-format": "^5.4.4",
    "react-parallax": "^3.5.2",
    "react-placeholder": "^4.1.0",
    "react-svg": "^16.3.0",
    "sass": "^1.93.2",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "tss-react": "^4.9.19",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251011.0",
    "@opennextjs/cloudflare": "^1.11.0",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.9.1",
    "@types/paystack__inline-js": "^1.0.2",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@types/react-google-recaptcha": "^2.1.9",
    "@types/react-gtm-module": "^2.0.4",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "wrangler": "^4.43.0"
  },
  "trustedDependencies": [
    "@parcel/watcher",
    "unrs-resolver"
  ]
}
